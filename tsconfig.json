{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": ".",
    "paths": {
      // "@utils/*": ["../utils/*"],
      "@PageSetup": ["tests/setup/PageSetup.ts"],
      "@PageFactory": ["tests/setup/PageFactory.ts"],
      "@TestUtils": ["tests/utils/TestUtils.ts"],
      "@AssertUtils": ["tests/utils/AssertUtils.ts"],
      "@Timeouts": ["tests/utils/Timeouts.ts"],
      "@Types": ["tests/setup/Types.ts"]
    }
  }
}
